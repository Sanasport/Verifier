{
    "name": "arachne/verifier",
    "description": "Requirements verification for nette/application actions and components.",
    "type": "library",
    "keywords": [ "nette", "arachne", "security" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jáchym Toušek",
            "homepage": "http://enumag.cz",
            "email": "enumag@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Sanasport/ServiceCollections.git"
        }
    ],
    "require": {
        "php": "^7.1.0 || ~8.0.0 || ~8.1.0",
        "arachne/service-collections": "dev-master",
        "nette/application": "^3.1",
        "nette/di": "^3.0",
        "nette/utils": "^3.2",
        "doctrine/annotations": "^1.3.0",
        "latte/latte": "^2.9.0",
        "nettrine/annotations": "^0.7.0"
    },
    "require-dev": {
        "contributte/codeception": "^1.3",
        "codeception/codeception": "^4.1",
        "eloquent/phony": "^5.0",
        "eloquent/phony-phpunit": "^7.1",
        "friendsofphp/php-cs-fixer": "^2.8.0",
        "dg/bypass-finals": "^1.3.0",
        "nette/caching": "^3.0.2",
        "nettrine/cache": "^0.3.0"
    },
    "autoload": {
        "psr-4": {
            "Arachne\\Verifier\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Unit\\": "tests/unit/src",
            "Tests\\Functional\\": "tests/functional/src"
        },
        "files": [
            "vendor/codeception/codeception/shim.php"
        ]
    },
    "scripts": {
        "ci": [
            "@fix",
            "@analyse",
            "@test"
        ],
        "fix": "\"vendor/bin/php-cs-fixer\" fix",
        "analyse": "\"vendor/bin/phpstan\" analyse --no-progress --level=max --configuration=phpstan.neon src tests",
        "test": [
            "\"vendor/bin/codecept\" build",
            "\"vendor/bin/codecept\" run --debug"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.10-dev"
        }
    }
}
